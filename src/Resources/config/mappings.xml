<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="psi_content_type.mapping.string" class="Psi\Component\ContentType\Mapping\StringMapping">
            <tag name="psi_content_type.mapping" alias="string" />
        </service>

        <service id="psi_content_type.mapping.datetime" class="Psi\Component\ContentType\Mapping\DateTimeMapping">
            <tag name="psi_content_type.mapping" alias="datetime" />
        </service>

        <service id="psi_content_type.mapping.reference" class="Psi\Component\ContentType\Mapping\ReferenceMapping">
            <tag name="psi_content_type.mapping" alias="reference" />
        </service>

    </services>

</container>
