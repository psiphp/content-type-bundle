<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="psi_content_type.field.text" class="Psi\Component\ContentType\Standard\Field\TextField">
            <tag name="psi_content_type.field" alias="text" />
        </service>

        <service id="psi_content_type.field.datetime" class="Psi\Component\ContentType\Standard\Field\DateTimeField">
            <tag name="psi_content_type.field" alias="datetime" />
        </service>

        <service id="psi_content_type.field.collection" class="Psi\Component\ContentType\Standard\Field\CollectionField">
            <argument type="service" id="psi_content_type.registry.field" />
            <tag name="psi_content_type.field" alias="collection" />
        </service>

    </services>

</container>


